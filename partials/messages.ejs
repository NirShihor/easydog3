<% if(typeof errors !== 'undefined') { %> <% errors.forEach(function(error) { %>
<div class="alert alert-warning alert-dismissible fade show" role="alert">
  <%= error.msg %>
</div>
<% }); %> <% } %> <% if (success_msg) { %>
<div><%= success_msg %></div>
<% } %> <% if (error_msg != '') { %>
<div class="alert alert-warning alert-dismissible fade show" role="alert">
  <%= error_msg %>
</div>
<% } %>
